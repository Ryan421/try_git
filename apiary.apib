                {"id":"OutCount","type":"number"},
                {"id":"OutSize","type":"number"},
                {"id":"InternalCount","type":"number"},
                {"id":"InternalSize","type":"number"}],
                "data":[
                [1353456000000,0,0,0,0,0,0],
                [1353542400000,2,2,0,0,0,0]
